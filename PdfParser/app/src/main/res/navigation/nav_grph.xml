<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_grph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.hackygirls.pdfparser.fragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_yourDocFragment"
            app:destination="@id/yourDocFragment" />
    </fragment>
    <fragment
        android:id="@+id/yourDocFragment"
        android:name="com.hackygirls.pdfparser.fragments.YourDocFragment"
        android:label="fragment_your_doc"
        tools:layout="@layout/fragment_your_doc" />
</navigation>